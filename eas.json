{
  "cli": {
    "version": ">= 16.20.4",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "beta": {
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "beta": {
      "android": {
        "track": "beta",
        "serviceAccountKeyPath": ".github/keys/play.json"
      }
    },
    "production": {
      "android": {
        "track": "production",
        "serviceAccountKeyPath": ".github/keys/play.json"
      }
    }
  }
}
